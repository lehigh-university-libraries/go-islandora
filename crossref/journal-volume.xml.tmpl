<doi_batch xmlns="http://www.crossref.org/schema/5.3.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.3.1" xsi:schemaLocation="http://www.crossref.org/schema/5.3.1 https://data.crossref.org/schemas/crossref5.3.1.xsd" xmlns:jats="http://www.ncbi.nlm.nih.gov/JATS1">
  <head>
    <doi_batch_id>{{ .Head.BatchId }}</doi_batch_id>
    <timestamp>{{ .Head.Timestamp }}</timestamp>
    <depositor>
      <depositor_name>{{ .Head.Depositor.Name }}</depositor_name>
      <email_address>{{ .Head.Depositor.Email }}</email_address>
    </depositor>
    <registrant>{{ .Head.Registrant }}</registrant>
  </head>
  <body>
  {{- range .JournalVolume }}
    <journal>
      <journal_metadata language="en">
        <full_title>{{ .JournalTitle }}</full_title>
        <doi_data>
          <doi>{{ .JournalDoiData.Doi }}</doi>
          <resource>{{ .JournalDoiData.Url }}</resource>
        </doi_data>
      </journal_metadata>
      <journal_issue>
        <publication_date media_type="online">
          <year>{{ .Year }}</year>
        </publication_date>
        <journal_volume>
          <volume>{{ .Number }}</volume>
          <doi_data>
            <doi>{{ .VolumeDoiData.Doi }}</doi>
            <resource>{{ .VolumeDoiData.Url }}</resource>
          </doi_data>
        </journal_volume>
      {{- if .Issue }}
      <issue>{{ .Issue }}</issue>
      {{- end }}
      </journal_issue>
    {{- range .Articles }}
      <journal_article publication_type="full_text">
        <titles>
          <title>{{ .Title }}</title>
        </titles>
      {{- if .Contributors }}
        <contributors>
        {{- range .Contributors }}
          <person_name sequence="{{ .Sequence }}" contributor_role="{{ .Role }}">
          {{- if .Name.Given }}
            <given_name>{{ .Name.Given }}</given_name>
          {{- end }}
            <surname>{{ .Name.Surname }}</surname>
          {{- if .Name.Institution }}
            <affiliation>{{ .Name.Institution }}</affiliation>
          {{- end }}
          {{- if .Name.ORCID }}
            <ORCID>{{ .Name.ORCID }}</ORCID>
          {{- end }}
          </person_name>
          {{- if .Organization }}
          <organization sequence="additional" contributor_role="{{ .Role }}">{{ .Organization }}</organization>
          {{- end }}
        {{- end }}
        </contributors>
      {{- end }}
      {{- if .LicenseRef }}
        <license>
          <license_ref>{{ .LicenseRef }}</license_ref>
        </license>
      {{- end }}
        <publication_date media_type="online">
          <year>{{ .Year }}</year>
        </publication_date>
        <doi_data>
          <doi>{{ .DoiData.Doi }}</doi>
          <resource>{{ .DoiData.Url }}</resource>
        </doi_data>
      </journal_article>
    {{- end }}
    </journal>
  {{- end }}
  </body>
</doi_batch>
